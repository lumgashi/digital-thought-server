// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("db.url")
}


model AutoId {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  autoId     Int
  userAutoId Int
  bidAutoId  Int    @default(0)

  @@map(name: "autoIds")
}